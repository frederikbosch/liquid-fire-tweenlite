{{#link-to 'about' tagName="button" class="btn btn-info btn-large"}}View The Transition Map{{/link-to}}
<h2>Index</h2>
<table class="table">
  <thead>
  <tr>
      <th>#</th>
      <th>Transition</th>
      <th>Effect</th>
  </tr>
  </thead>
  <tbody>
  {{#link-to 'transitions.cross-fade' tagName="tr"}}
      <td>1</td><td>Cross Fade</td><td>Fade Out while Fading In</td>
  {{/link-to}}
  {{#link-to 'transitions.default' tagName="tr"}}
      <td>2</td><td>Default</td><td>None</td>
  {{/link-to}}
  {{#link-to 'transitions.fade' tagName="tr"}}
      <td>3</td><td>Fade</td><td>Fade Out, then Fade In</td>
  {{/link-to}}
  {{#link-to 'transitions.flex-grow' tagName="tr"}}
      <td>4</td><td>Flex Grow</td><td>It Flexes</td>
  {{/link-to}}
  {{#link-to 'transitions.fly-to' tagName="tr"}}
      <td>5</td><td>Fly To</td><td>(fly to)</td>
  {{/link-to}}
  {{#link-to 'transitions.move-over' tagName="tr"}}
      <td>6</td><td>Move Over</td><td>Moves Content Over</td>
  {{/link-to}}
  {{#link-to 'transitions.scale' tagName="tr"}}
      <td>7</td><td>Scale</td><td>It Scales</td>
  {{/link-to}}
  {{#link-to 'transitions.scroll-then' tagName="tr"}}
      <td>8</td><td>Scroll Then</td><td>Scrolls to the top first.</td>
  {{/link-to}}
  {{#link-to 'transitions.to-down' tagName="tr"}}
      <td>9</td><td>To Down</td><td>It Goes Up</td>
  {{/link-to}}
  {{#link-to 'transitions.to-left' tagName="tr"}}
      <td>10</td><td>To Left</td><td>It Goes Right</td>
  {{/link-to}}
  {{#link-to 'transitions.to-right' tagName="tr"}}
      <td>11</td><td>To Right</td><td>It Goes Left</td>
  {{/link-to}}
  {{#link-to 'transitions.to-up' tagName="tr"}}
      <td>12</td><td>To Up</td><td>It Goes Down</td>
  {{/link-to}}
  </tbody>


</table>